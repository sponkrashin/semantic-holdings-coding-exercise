{
  "name": "accounting-dashboard-web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "eslint src/**/*.ts src/**/*.html",
    "lint:fix": "eslint src/**/*.ts src/**/*.html --fix"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
