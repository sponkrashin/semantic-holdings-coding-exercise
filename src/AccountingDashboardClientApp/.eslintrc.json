{
    "root": true,
    "plugins": [
        "@angular-eslint"
    ],
    "overrides": [
        {
            "files": [
                "*.ts"
            ],
            "extends": [
                "plugin:@angular-eslint/recommended"
            ],
            "rules": {}
        },
        {
            "files": [
                "*.html"
